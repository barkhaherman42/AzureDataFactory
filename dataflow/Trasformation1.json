{
	"name": "Trasformation1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "StagingParquet2",
						"type": "DatasetReference"
					},
					"name": "source1",
					"script": "source(allowSchemaDrift: true,\n\tvalidateSchema: false) ~> source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ParquetDestinationStore2",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"script": "source1 sink(allowSchemaDrift: true,\n\tvalidateSchema: false) ~> sink1"
				}
			]
		}
	}
}