{
	"name": "DataflowCopy1",
	"properties": {
		"description": "Run dataflow with Explicit copy to ingest copy source",
		"activities": [
			{
				"name": "LoadData",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "DocumentDbCollectionSource",
						"nestingSeparator": "."
					},
					"sink": {
						"type": "AzureBlobFSSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"id": "id",
							"name": "name",
							"property1": "property1",
							"property2": "property2",
							"_rid": "_rid",
							"_self": "_self",
							"_etag": "_etag",
							"_attachments": "_attachments",
							"_ts": "_ts"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "CosmosDbSourceDatasetEntity2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureDataLakeStorageFile1",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Trasformation",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "LoadData",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataflow": {
						"referenceName": "Trasformation1",
						"type": "DataFlowReference"
					}
				}
			}
		]
	}
}